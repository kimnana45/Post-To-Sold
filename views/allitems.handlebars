<div class="container">  
    <div class="row">
      {{#each items}}
      <div class="col s12 m4">
        <div id="cms" class="card">
          <div class="card-image">
            <img src={{dataValues.picture}}>
            <span class="card-title">{{dataValues.name}}</span>
          </div>
          <div class="card-content">
            <p>{{dataValues.price}}</p>
            <hr class="my-4">
            <p class="description">{{dataValues.description}}</p>
            <hr class="my-4">
            <p>Category: {{dataValues.category}}</p>
            <hr class="my-4">
            <p>Sold by: {{User.dataValues.email}}</p>
            <hr class="my-4">
            <button class="purchase-btn waves-effect waves-light"><i class="fas fa-hand-holding-usd"></i>Purchase Item</button>
          </div>
        </div>
      </div>
      {{/each}}
  </div>
  

<script type="text/javascript" src="/js/allitems.js"></script>